<Project Sdk="Microsoft.NET.Sdk">

	<PropertyGroup>
		<TargetFrameworks>net45;net46;netstandard1.0;netstandard1.3;netstandard2.0;netstandard2.1;net5.0</TargetFrameworks>
		<PackageId>EVL</PackageId>
		<Version>0.0.1</Version>
		<Authors>Saran Tanpituckpong</Authors>
		<Product>EVL.Net</Product>
		<Description>Function fallback when error.</Description>
		<PackageTags>evl;error</PackageTags>
		<PackageProjectUrl>https://github.com/gluons/EVL.NET</PackageProjectUrl>
		<PackageLicenseExpression>MIT</PackageLicenseExpression>
		<PublishRepositoryUrl>true</PublishRepositoryUrl>
		<GenerateDocumentationFile>true</GenerateDocumentationFile>
	</PropertyGroup>

</Project>
